    "server-only": "^0.0.1",
    "undici": "^7.2.0",
    "ws": "^8.18.0",
    "@t3-oss/env-nextjs": "^0.11.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@undrstnd/typescript-config": "workspace:*",
    "@types/node": "22.10.5",
    "@types/ws": "^8.5.13",
    "bufferutil": "^4.0.9",
    "prisma": "6.1.0",
    "typescript": "^5.7.2"
